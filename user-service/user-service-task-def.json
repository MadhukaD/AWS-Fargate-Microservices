{
  "family": "user-service-task",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "user-service",
      "image": "348184816643.dkr.ecr.ap-southeast-1.amazonaws.com/user-service:latest",
      "memoryReservation": 512,
      "cpu": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3001,
          "hostPort": 3001,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::348184816643:role/fargate-microservices-ecs-task-execution-role"
}